{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Launch server",
        "type": "php",
        "request": "launch",
        "program": "${workspaceRoot}/clean.php",
        "runtimeArgs": [
          "-dxdebug.client_port=9000",
          "-dxdebug.mode=develop,debug",
          "-dxdebug.start_with_request=yes",
          "-S",
          "127.0.0.1:8000"
        ],
        "xdebugSettings": {
          "max_data": -1, "max_children": 1000
        },
        
        "cwd": "${workspaceRoot}",
        "port": 9000,
        "serverReadyAction": {
          "pattern": "Development Server \\(http://127.0.0.1:([0-9]+)\\) started",
          "uriFormat": "http://localhost:%s",
          "action": "openExternally"
        }
      }
    ]
  }